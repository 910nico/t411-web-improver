{
	"name": "T411-Improver",
	"version": "13.1.8.1",
	"manifest_version": 2,
	"description": "Extension pour t411.me qui am√©liore certains aspects du site.",
	"icons": { 
		"16": "logo16.png",
		"48": "logo48.png",
		"128": "logo128.png" },



	"content_scripts": [ 
		{ 
			"matches": ["http://www.t411.me/torrents/upload-step-2/?category=433*"], 
			"js": ["mediainfo.js", "video-upload.js"] 
		}, { 
			"matches": ["http://www.t411.me/torrents/upload-step-2/?category=631*"], 
			"js": ["mediainfo.js", "video-upload.js"] 
       }, { 
			"matches": ["http://www.t411.me/torrents/search/*"], 
			"js": ["fast-dl.js", "mass-signal.js"] 
       }, { 
			"matches": ["http://forum.t411.me/*"], 
			"all_frames": true, 
			"js": ["profile-forum.js"] 
       }],

	"permissions": [
		"http://www.t411.me/torrents/upload-step-2/*",
		"http://www.t411.me/torrents/search/"
	],
	"web_accessible_resources": [
		"mediainfo-langs.csv",
		"Download-64.png",
		"Contact-32.png"
	]
}
